			String instruction = "";
			String strChargeType = "3000";
			int cntUser = 0;
			boolean blnChgUseType = true;
			int useType = 0;
			
			
			// 231201 ratelamsh / 안내 문구 만들 떄 StringBuilder -> StringJoiner 변경
			StringJoiner sj = new StringJoiner(" / ", "<선거>로 변경 불가 - ", "");


			// 정액형 상품 유저 or 선거 계정 있는 회원 안내 문구 출력
			StringBuilder sb = new StringBuilder();
			
			
			// OBS 정액형 상품 사용중인 유저는 변경 불가
			if(strChargeType.substring(0,3).equals("OBS")) {
				
				sj.add("정액형 상품 사용 중");	
				blnChgUseType = false;
			}

			// 일반 회원인데 동일한 명의에 선거 계정이 있는 경우
			if(useType == 0) {
				// regcode와 company.name이 같은 회원이 있는지 쿼리문으로 검사
				//cntUser = rs.getInt(1);
				if(cntUser > 0) blnChgUseType = false; sj.add("선거 계정 존재함 (회원별 선거 계정은 1개만 가능)");
				
			}

			//instruction = sb.toString();
			instruction = sj.length() > 14 ? sj.toString() : "";
			System.out.println(blnChgUseType);
			System.out.println("instruction : " + instruction);
			
			
			
			
			

	}	// main end
	


	// 임시 폴더 생성 
	public static void createTempFolder(String tempPath) {
		
		File folder = new File(tempPath);
		try {
			if(folder.exists()) {
				System.out.println("demo 임시 폴더가 존재합니다. 하위 파일을 삭제합니다.");
				delFiles(folder);
			} else {
				folder.mkdir();
				System.out.println("demo 임시 폴더 생성");
			}
		} catch(Exception e) {
			e.printStackTrace();
		}
	}
	
	
	// 하위 파일 삭제
	public static void delFiles(File folder) {
		File[] folder_list = folder.listFiles();
		for(File file : folder_list) {
			file.delete();
			System.out.println(file + " 삭제 완료");
		}
	}
	
	// fileno로 특정 파일 삭제(물리적)
	public static void delFilePhs(String filePath) throws Exception {
		File f = new File(filePath);
		f.delete();
		System.out.println(filePath + " 삭제 완료");
	}
	
}
