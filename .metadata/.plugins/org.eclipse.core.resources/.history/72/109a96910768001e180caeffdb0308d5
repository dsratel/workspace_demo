		System.out.println("MemberDAOImpl memberDto : " + memberDto);
		Date date = sqlsession.selectOne("memberMapper.test_now");
		System.out.println("date : " + date);
		//return sqlsession.insert("memberMapper.insertMember", memberDto);
		return 0;
		
	}

}
