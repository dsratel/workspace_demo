package com.dialoguespace.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestBody;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.servlet.config.annotation.EnableWebMvc;

import com.dialoguespace.dto.MemberDTO;
import com.dialoguespace.service.MemberService;

@EnableWebMvc
@Controller
@RequestMapping("/member")
public class MemberController {
	
	private MemberService memberService;
	
	@RequestMapping("/insertMember.do")
	public int insertMember(@RequestBody MemberDTO memberDto) throws Exception {
		return memberService.insertMember(memberDto);
	}

}
